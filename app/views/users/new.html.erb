<%= form_with(model: @user_form, url: users_path, local: true) do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>
  <%= form.label :email %>
  <%= form.email_field :email %>
  <%= form.label :password %>
  <%= form.password_field :password %>
  <%= form.label :password_digest %>
  <%= form.password_field :password_digest %>
  <%= form.submit "Log in" %>
<% end %>